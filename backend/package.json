{
  "name": "nodejs-core",
  "version": "1.0.0",
  "description": "nodejs config template",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/bin/www --exec babel-node",
    "build": "babel src --out-dir dist",
    "start": "NODE_ENV=production node dist/bin/www",
    "test": "NODE_ENV=test jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watch-all",
    "test:coverage": "jest --config ./jest.config.js --coverage"
  },
  "keywords": [],
  "author": "@kevfarid",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "babel-core": "^6.26.3",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6"
  }
}
